

[] (DainaCompiler, DainaCommandInterface, ControlFlow, DainaStatusReporter) 
{
	^ *{
		[DainaCommandInterface] dainaCommandInterface = \[DainaCommandInterface]:newDainaCommandInterface;
		[DainaStatusReporter] statusReporterForCompiler = \dainaCommandInterface:provideDainaStatusReporterWhichReportsToThisCommandInterface;
		[DainaCompiler] dainaCompiler = \[DainaCompiler]:newDainaCompilerReadyToReceiveDainaCommandsWithReporterToReportCompilerStatus statusReporterForCompiler;
		\[ControlFlow]:repeatUntilStopped sendValidCommandsFromCommandInterfaceToDainaCompilerAndNeverStop;
	}	!{
			[[->]->] sendValidCommandsFromCommandInterfaceToDainaCompilerAndNeverStop = *([]_){
				\dainaCommandInterface:processNextUserCommandAndSendingValidCommandsToCommandReceiver dainaCompiler;
			}
		}
}









