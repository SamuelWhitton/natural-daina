      } !{
            [Integer] index = \currentIndex:get;
            [?] element = \sequence:atIndex index;
         })@asdf ! binding
   }

   :: while *([->[Boolean]] condition, [->] action) {
      \[?:]:until *([->] stop) {
         \(\condition):ifOrElse action stop
      }
   }

   :: until *([[->]->] actionWithStop) {
